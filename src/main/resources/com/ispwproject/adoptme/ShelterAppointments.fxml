         <VBox.margin>
            <Insets left="20.0" />
         </VBox.margin>
      </ScrollPane>
      <Label layoutX="10.0" layoutY="355.0" text="Confirmed Requests">
         <font>
            <Font name="Arial Bold" size="18.0" />
         </font>
         <VBox.margin>
            <Insets top="10.0" />
         </VBox.margin>
      </Label>
      <ScrollPane fitToHeight="true" hbarPolicy="NEVER" layoutX="10.0" layoutY="480.0" minHeight="-Infinity" prefHeight="202.0" styleClass="transparent" vbarPolicy="NEVER" VBox.vgrow="NEVER">
         <content>
            <HBox fx:id="confirmedReqList" />
         </content>
         <VBox.margin>
            <Insets left="20.0" />
         </VBox.margin>
      </ScrollPane>
   </children>
   <padding>
      <Insets left="20.0" top="20.0" />
   </padding>
</VBox>
