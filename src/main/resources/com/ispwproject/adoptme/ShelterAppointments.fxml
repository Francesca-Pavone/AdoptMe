<VBox alignment="CENTER_LEFT" prefHeight="710.0" prefWidth="790.0" xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.ispwproject.adoptme.controller.graficcontroller.gui.GUIShelterAppointmentsController">
   <children>
      <Label text="Sent Requests">
