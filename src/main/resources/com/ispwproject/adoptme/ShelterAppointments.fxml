            <HBox fx:id="sentReqList" />
         </content>
         <VBox.margin>
            <Insets left="20.0" />
         </VBox.margin>
      </ScrollPane>
      <Label text="Pending Requests">
         <font>
            <Font name="Arial Bold" size="18.0" />
         </font>
         <VBox.margin>
            <Insets top="10.0" />
         </VBox.margin>
      </Label>
      <ScrollPane fitToHeight="true" hbarPolicy="NEVER" layoutX="30.0" layoutY="31.0" minHeight="-Infinity" prefHeight="202.0" styleClass="transparent" vbarPolicy="NEVER" VBox.vgrow="NEVER">
